public CountDown()
{
	if (Count > 0)
	{
		GameTextForAll( CountText[Count-1], 2500, 3);
		Count--;
		SetTimer("CountDown", 1000, 0);
	}
	else
	{
		GameTextForAll("~w~Go!!!", 2500, 3);
		Count = 5;
	}
	return 1;
}

