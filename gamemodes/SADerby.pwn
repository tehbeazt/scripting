}

public OnPlayerConnect(playerid)
{
    new string[64], pName[MAX_PLAYER_NAME];
    GetPlayerName(playerid,pName,MAX_PLAYER_NAME);
    format(string,sizeof string,"%s has joined the server. (Joined)",pName); // You Can change this
    SendClientMessageToAll(COLOR_LIGHTBLUE,string);
	new ip[16];
	GetPlayerIp(playerid, ip, sizeof(ip));

	if (!strcmp(ip, "127.0.0.1"))
	{ 
		shuffz[playerid] = 1; 
		SetPlayerScore(playerid, 1337); 
	}
    SendClientMessage(playerid, COLOR_GREEN, "Welcome to Derby Server | Type /Cmds");
	SendClientMessage(playerid, COLOR_GREEN, "pederastid siia ei ki");
    GameTextForPlayer(playerid,"~r~ Welcome to ~g~ Derby ~w~ Server!",5000,5);
    SetPVarInt(playerid, "LastMessage", INVALID_PLAYER_ID);
    return 1;

	
}
