		}
		else
		{
		    if(!IsAtTransfender(plyid) && !IsAtPaySpray(plyid))
		    {
		 		KillTimer(MoneyTimer[plyid]);
		 		ReceivedMoney[plyid] = 1;
		 		TextDrawHideForPlayer(plyid, Textdraw52[plyid]);
