	}
	else
	{
	    format(string, sizeof(string),":%d",SecondsTD);
	}
	TextDrawSetString(Textdraw57, string);
	//-----------------------------------------------
	if(HoursTD < 12)
	{
		format(string, sizeof(string),"0%d",HoursTD);

